	New()
		for(var/xi=monster_x_start,xi<=monster_x_end,xi++)
			for(var/yi=monster_y_start,yi<=monster_y_end,yi++)
				if(xi||yi)
					var/obj/O = new()
					O.icon_state="[xi],[yi]"
					if(xi) O.pixel_x = (32*xi)
					if(yi) O.pixel_y = (32*yi)
					overlays+=O
		..()
	Del(var/mob/Killer)
		//locating battlefield the monster is on
		var/turf/battle/location/BLoc = locate(/turf/battle/location/) in view()
		//monster's drop
		if(istype(Killer,/mob/PC))
			//key item drop
			if(obj_reward)
				for(var/mob/PC/p in view(BLoc))
					var/obj/O = locate(obj_reward) in p.contents
					if(O) continue
					O = locate(obj_reward) in p.iteminbank
					if(O) continue
					O = new obj_reward
					BLoc.obj_reward[O] = p
