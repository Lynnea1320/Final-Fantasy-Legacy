				F.cd = "/config/"
				F["defeated_bosses"]<<defeated_bosses
		*/
			//random drop
			if(prob(drop_rate))
				var/obj/O
				switch(rand(0,32))
					if(1 to 11) O = obj_drop[2]
					if(12 to 32) O = obj_drop[1]
					else O = obj_drop[3]
				if(O)
					O = new O
					BLoc.obj_reward[O] = Killer
		//removing monster
		for(var/obj/onmap/dmgnum/O in view(BLoc)) if(O.screentag==src) del(O)
		overlays=list()
		..()
